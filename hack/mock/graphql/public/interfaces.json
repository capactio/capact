              "action": {
                "runnerInterface": "cap.interface.runner.argo",
                "args": {
                  "workflow": {
                    "volumes": [
                      {
                        "name": "gcp-credentials",
                        "secret": {
                          "secretName": "gcp-credentials"
                        }
                      }
                    ],
                    "entrypoint": "jira",
                    "templates": [
                      {
                        "name": "jira",
