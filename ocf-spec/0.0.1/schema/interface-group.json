{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "description": "InterfaceGroup stores metadata for a group of Interfaces.",
  "required": [
    "ocfVersion",
    "kind",
    "metadata"
  ],
  "properties": {
    "ocfVersion": {
      "$id": "#/properties/ocfVersion",
      "type": "string",
      "const": "0.0.1"
    },
    "kind": {
      "$id": "#/properties/kind",
      "type": "string",
      "enum": [
        "InterfaceGroup"
      ]
    },
    "metadata": {
      "$ref": "https://capact.io/schemas/common/metadata.json"
    }
  }
}
